    "version": "0.2.0",
    "configurations": [

        {
            "name": "Mercadlibre",
            "type": "debugpy",
            "request": "launch",
            "program": "src/scrapers/mercadolibre.py",
            "console": "integratedTerminal"
        },

        {
            "name": "Main",
            "type": "debugpy",
            "request": "launch",
            "program": "src/main.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "python": "${workspaceFolder}/.venv/bin/python"
        }
    ]
}